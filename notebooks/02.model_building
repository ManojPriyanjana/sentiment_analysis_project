    "def save_vocabulary(lines,filename):\n",
    "    data='\\n'.join(lines)\n",
    "    file = open(filename,'w',encoding=\"utf-8\")\n",
    "    file.write(data)\n",
    "    file.close()\n",
    "\n",
    "save_vocabulary(tokens,'../static/model/vocabulary.txt')"
   ]
